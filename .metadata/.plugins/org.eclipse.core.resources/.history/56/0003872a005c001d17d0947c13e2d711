package com.tourist.model;

@
public class Tourist {
	
	private Long id;
	private String name;
	private String gender;
	private String age;
	private String fromplace;
	private String visitdays;
	
	
	public Tourist() {
		super();
	}
	
	
	public Tourist(String name, String gender, String age, String fromplace, String visitdays) {
		super();
		this.name = name;
		this.gender = gender;
		this.age = age;
		this.fromplace = fromplace;
		this.visitdays = visitdays;
	}


	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getGender() {
		return gender;
	}
	public void setGender(String gender) {
		this.gender = gender;
	}
	public String getAge() {
		return age;
	}
	public void setAge(String age) {
		this.age = age;
	}
	public String getFromplace() {
		return fromplace;
	}
	public void setFromplace(String fromplace) {
		this.fromplace = fromplace;
	}
	public String getVisitdays() {
		return visitdays;
	}
	public void setVisitdays(String visitdays) {
		this.visitdays = visitdays;
	}
	
	
}
